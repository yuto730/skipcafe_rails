<script>
  $("#dropdown_user_btn").on("click", function(){
    $("#dropdown_user_btn").toggleClass("transform");
  });
  $("#dropdown_news_btn").on("click", function(){
    $("#dropdown_news_btn").toggleClass("transform");
  });
